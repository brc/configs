  <!-- Disable Command+H (window hide) -->
  <item>
    <name>Disable "Hide" shortcut in Virtual Box</name>
    <appendix>Don't fucking enable Command+H</appendix>
    <identifier>private_dont_hide_vbox</identifier>
    <device_only>DeviceVendor::MASSDROP, DeviceProduct::ERGODOX</device_only>
    <only>VBOX</only>
    <autogen>
      __KeyToKey__
      KeyCode::H, ModifierFlag::COMMAND_L,
      KeyCode::H
    </autogen>
  </item>

